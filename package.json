{
    "name": "carbon-now-sh",
    "displayName": "carbon-now-sh",
    "description": "A Code package to open the current editor content in carbon.now.sh",
    "version": "0.0.1",
    "publisher": "ericadamski",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:carbon.show"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "carbon.show",
                "title": "Carbon"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "typescript": "^2.6.1"
    },
    "dependencies": {
        "vscode": "^1.1.6",
        "openurl": "^1.1.1"
    }
}
